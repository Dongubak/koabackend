{
   "env": {
      "commonjs": true,
      "es6": true,
      "node": true
   },
   "extends": ["eslint:recommended", "prettier"],
   "parserOptions": {
      "ecmaVersion": 2018
   },
   "rules": {
      "no-unused-vars": "warn",
      "nop-console" : "off"
   },
   "globals": {
      "process": "readonly"
   }
}